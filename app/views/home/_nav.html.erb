<div class="navbar navbar-default navbar-fixed-top" role="navigation">
  <div class="container">

    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target=".navbar-collapse">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="<%= root_path %>">
        <%= image_tag "logoW.png", title: "You Do U" %>
      </a>
    </div>

    <div class="collapse navbar-collapse">
      <ul class="nav navbar-nav">
        <li class="<% if current_page? root_path %>active<% end %>">
          <%= link_to 'Home', root_path %>
        </li>

        <li class="<% if current_page? root_path %>active<% end %>">
          <%= link_to 'Social', '/social/index' %>
        </li>

        <li class="<% if current_page? root_path %>active<% end %>">
          <%= link_to 'Academic', '/academic/index' %>
        </li>

        <li class="<% if current_page? events_path %>active<% end %>">
          <%= link_to 'Events', events_path %>
        </li>

        <li class="<% if current_page? about_path %>active<% end %>">
          <%= link_to 'About', about_path %>
        </li>
      </ul>

      <ul class="nav navbar-nav navbar-right">
        <% if user_signed_in? %>
          <li><%= link_to "Logged in as #{current_user.email}", root_path %></li>
          <li><%= link_to "Log Out", destroy_user_session_path, method: :delete %></li>
        <% else %>
          <li><%= link_to "Sign Up", new_user_registration_path %></li>
          <li><%= link_to "Sign In", new_user_session_path %></li>
        <% end %>
      </ul>
    </div><!--/.nav-collapse -->

  </div><!--/.container -->
