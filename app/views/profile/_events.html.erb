<h2>Events</h2>

<table class="table table-hover" style="margin: 2em 0 0 1em;">
  <% @my_events.each do |event| %>
    <tr>
      <td>
        <%= link_to event.name, event.url %><br>
        <%= event.date.strftime("%A, %b %d") %>
      </td>
      <td style="text-align: right">
        <%= render 'event/unfollow', event: event %>
      </td>
    </tr>
  <% end %>
</table>

